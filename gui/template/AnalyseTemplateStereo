#!/bin/csh
#$ -N RoundUp
#$ -e {5}
#$ -o {5}
#$ -l ct=24:00:00
#$ -l fsize=10240M
#$ -l sps=1
#$ -l xrootd=1
#$ -l mem_free=4096M
#$ -l s_vmem=4096M
#$ -p 0
#$ -l os=sl6
#$ -P P_hess -cwd


#source /afs/in2p3.fr/throng/hess/HESS_Soft_0-8-32/env.csh
source {6}/env.csh
awk 'BEGIN {srand();random=int(rand()*120); print random}' | xargs sleep

if ( ! $?TMPDIR ) setenv TMPDIR `pwd`

if ( ! $?TMPBATCH ) setenv TMPBATCH $TMPDIR

unsetenv HPSS
setenv XROOTD root://ccxroot:1999//hpss/in2p3.fr/group/hess
setenv CALIB_PATH /sps/hess/prod/Calibration
setenv SPECTRUM_PATH {7}
#setenv SPECTRUM_PATH /sps/hess/prod/SpectrumTables/HESS_Soft_0-8-32_HESSII_Prod8_FineSlice
setenv DST_XROOTD_PATH root://ccxroot:1999//hpss/in2p3.fr/group/hess/DST

#cd $TMPBATCH

cat > make_{8}_ModelPlus_Combined3_Std_run.C  << EOF
void make_{8}_ModelPlus_Combined3_Std_run(unsigned runno)
{
if(gROOT->IsBatch())
  gSystem->Load("libGui.so");
gROOT->SetStyle("Plain");
gStyle->SetOptFit(111);
gStyle->SetPalette(1);
//===================
// = Analysis 
gSystem->Load("librootparisanalysis.so");
gSystem->Load("librootparisrunquality.so");
gSystem->Load("librootparisanalysisCommon.so");
gSystem->Load("librootparisanalysis_scripts.so");
//=============================================

//===================
// = Path 
use_analysis();
set_verbose(false);
generate_stacktrace(false);


set_chain_name("ModelPlus_Combined3_Std");
//===================
// = Analysis Setup
use_model_analysis();
set_model_stereo_name("Hybrid",1,999);
use_model_mono_analysis();
set_model_mono_name("Mono");
use_model_hessI_analysis();
set_model_hessI_name("HESSI",1,999);
fill_likelihood_monitor(1);
save_likelihood_monitor(0);
set_prefered_reconstruction("ModelCombined");
//===================
// = DST Config
set_dst_type(SashFile::DST_Model);
set_dst_version("Model_HESSII_Hybrid_Prod8");
set_dst_subconfig("DEFAULT");
//===================
// = Shape Cuts
set_chargemin("ModelStereo",60,Sash::TelescopeConfig::HESS1_960);
set_nomdistance_range("ModelStereo",0,2, Sash::TelescopeConfig::HESS1_960);
set_min_containment("ModelStereo",0.5,Sash::TelescopeConfig::HESS1_960);
set_min_pixelsabove5pe("ModelStereo",5,Sash::TelescopeConfig::HESS1_960);
set_chargemin("ModelStereo",60,Sash::TelescopeConfig::HESS2_2048);
set_nomdistance_range("ModelStereo",0,2, Sash::TelescopeConfig::HESS2_2048);
set_min_containment("ModelStereo",0.5,Sash::TelescopeConfig::HESS2_2048);
set_min_pixelsabove5pe("ModelStereo",5,Sash::TelescopeConfig::HESS2_2048);
set_chargemin("ModelHESSI",60,Sash::TelescopeConfig::HESS1_960);
set_nomdistance_range("ModelHESSI",0,2, Sash::TelescopeConfig::HESS1_960);
set_min_containment("ModelHESSI",0.5,Sash::TelescopeConfig::HESS1_960);
set_min_pixelsabove5pe("ModelHESSI",5,Sash::TelescopeConfig::HESS1_960);
set_chargemin("ModelMono",60,Sash::TelescopeConfig::HESS2_2048);
set_nomdistance_range("ModelMono",0,1.4, Sash::TelescopeConfig::HESS2_2048);
set_min_containment("ModelMono",0.5,Sash::TelescopeConfig::HESS2_2048);
set_min_pixelsabove5pe("ModelMono",5,Sash::TelescopeConfig::HESS2_2048);
//===================
// = Target 
set_radec();
set_target("{0}",{1},{2});
//===================
// = Run List 
set_run({3});
//===================
// = Regions
set_excludedregion_size(0.25);
//===================
// = Plots 
add_plot("MeanScaledShowerGoodness",150,-5,10,ParisAnalysis::Event_PreSelected,"",1);
